@using OutlookInspired.Blazor.Server.Editors

@if (ComponentModel?.Bytes is {Length:> 0 }){
    <embed id="pdfEmbed" type="application/pdf" style="@(ComponentModel.Style??"width:100%; height:100vh;")" 
           src="data:application/pdf;base64,@Convert.ToBase64String(ComponentModel.Bytes)#toolbar=@(ComponentModel.ToolBar ? 1 : 0)"/>
}
@code {
    [Parameter]
    public PdfModel ComponentModel { get; set; }
}