@inject IJSRuntime JS;
@using OutlookInspired.Blazor.Server.Components.DevExtreme.Maps
@using OutlookInspired.Blazor.Server.Services
@using OutlookInspired.Blazor.Server.Services.Internal
<DxVectorMap ComponentModel="ComponentModel" ></DxVectorMap>
@code {
    [Parameter]
    public DxVectorMapModel ComponentModel { get; set; }

    protected override async Task OnAfterRenderAsync(bool firstRender){
        if (!firstRender)return;
        ComponentModel.Options.Height = await JS.ModalBodyHeight();
    }


}